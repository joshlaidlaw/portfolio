{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "My website",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "npm run build:nunjucks && vite",
    "build": "npm run build:nunjucks && vite build",
    "build:nunjucks": "node scripts/build-nunjucks.js",
    "preview": "vite preview",
    "critical": "npm run build && node scripts/critical.js",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,json,scss,css,html,nunjucks}\"",
    "format:check": "prettier --check \"**/*.{js,json,scss,css,html,nunjucks}\"",
    "analyze": "ANALYZE=true vite build"
  },
  "author": "josh.laidlaw@gmail.com",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "critical": "^7.2.1",
    "eslint": "^9.39.1",
    "modern-normalize": "^3.0.1",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.47",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.96.0",
    "terser": "^5.44.1",
    "vite": "^7.2.7",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "dependencies": {
    "swiper": "^12.0.3"
  }
}
